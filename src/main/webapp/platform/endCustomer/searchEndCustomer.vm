<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
	<head>
		<meta http-equiv="Content-Type" content="text/html; charset=gbk" />
		#CSSM(["easyui","style"])
		#JSM(["jquery", "easyui"])
		#JS_ENV_VAL()
	</head>
	<body>
		<!-- ²éÑ¯ -->
		<div class="easyui-layout" data-options="fit:true,border:false"> 
			<input type="hidden" id="ids"/>
	        <form method="post" target="hideFrame">
			<div id="p_search"  border="0" title="" style="height:90px;padding: 2px;" data-options="region:'north',iconCls:'icon-search'">
				<br>
				<table width="100%" border="0" cellpadding="0" cellspacing="1">
					<input  type="hidden"  id="loginRole" value="$!{loginRole}">
				<tr>
                    <td class="head" noWrap >Disti</td>
                    <td width='120'>
						<input class="easyui-validatebox" id="disti_groupId" name="disti_groupId" value="$!{disti_groupId}">
					</td>
 					<td class="head" noWrap width='120'>Customer Name</td>
                    <td><input class="easyui-validatebox" id="end_customer_name" name="end_customer_name" ></td>
						<td class="head" noWrap width='120'>Customer Group</td>
                    <td><input class="easyui-validatebox" id="end_customer_groupName" name="end_customer_groupName"></td>
					</tr>
					<tr>
						<td class="head" noWrap width='100'>Country</td>
                        <td  width='200'><input class="easyui-validatebox" id="country" name="country"></td>
    					<td class="head" noWrap width='100'>State/Province</td>
                        <td  width='200'><input class="easyui-validatebox" id="province" name="province"></td>
    					<td class="head" noWrap width='100'>Customer Type</td>
                        <td  width='200'>
							<select id="customer_type" name="customer_type" value=""style="width:155px;" data-options="editable:false">
	    						<option value=""></option>
	    						<option value="REP">REP</option>  
								<option value="DISTI">DISTI</option>  
	    						<option value="OEM">OEM</option>
	    						<option value="CEM">CEM</option>
							</select>
                        </td>
					</tr>
					<tr>
					<td class="head" noWrap width='120'>Status</td>
                    <td>
					<select id="states" name="states" value="(0,1,2)"style="width:155px;" data-options="editable:false">
						<option value="(0,1,2)">All</option>  
						<option value="(0)">Pending</option>  
						<option value="(1)">Approved</option>
						<option value="(2)">Reject</option>
						<option value="(3)">Freezed</option>
                    </select>
					</td>
					
					<td class="head" noWrap width='120'>Excluded Business Date</td>
					
					<td>
						<table>
							<tr>
								<td><input class="easyui-datebox"  id="noTranBegin" name="noTranBegin"></td>
								<td><input class="easyui-datebox"  id="noTranEnd" name="noTranEnd"></td>
							</tr>
						</table>
						
						
					</td>
					<td colspan="2">
						
					
						<a id="search" href="javascript:search()" class="easyui-linkbutton" data-options="iconCls:'icon-search'">Search</a>
                    	<a id="clearV" href="javascript:clearValue()" class="easyui-linkbutton" iconCls="icon-clear">Cancel</a> 	
					</td>		
					
                </tr>
                </table>
				 </div>
			    <div data-options="region:'center'">
					<table id="datagrid" border="0"></table>
				</div>
	</form>
	</div>
	<iframe id="hideFrame" name="hideFrame"  style="height: 0; width: 0"></iframe>
	<div id="hiddenWin"/> 
	</div>
	</body>
<script type="text/javascript" src="$!{env.imgUrl}/js/platform/endCustomer/searchEndCustomer.js"></script>
</html>